# Secret key for session management - use a unique value
SECRET_KEY=development-secret-key-for-testing

# Database configuration
DATABASE_URL=

# CORS origins (comma-separated values)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Cloudinary config - leave empty for development
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# Flask configuration
FLASK_ENV=development
FLASK_APP=app
FLASK_DEBUG=1
